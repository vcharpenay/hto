#################################################################


###  http://project-haystack.org/hto#associatedTo

hto:associatedTo rdf:type :AnnotationProperty ;
                 
                 rdfs:comment "gives the OWL class defined in the present ontology that should be associated to the individual defined as subject." ;
                 
                 rdfs:domain hto:HTag .





#################################################################
#
#    Object Properties
#
#################################################################


###  http://project-haystack.org/hto#belongsTo

hto:belongsTo rdf:type :ObjectProperty ;
              
              rdfs:range hto:Equipment ;
              
              rdfs:domain hto:Point ;
              
              rdfs:subPropertyOf <http://purl.oclc.org/NET/ssnx/ssn#onPlatform> .



###  http://project-haystack.org/hto#hasRef

hto:hasRef rdf:type :ObjectProperty ;
           
           rdfs:subPropertyOf :topObjectProperty .



###  http://project-haystack.org/hto#hasSection

hto:hasSection rdf:type :ObjectProperty ;
               
               rdfs:domain hto:Equipment ;
               
               rdfs:range hto:Section .



###  http://project-haystack.org/hto#hasTag

hto:hasTag rdf:type :ObjectProperty .



###  http://project-haystack.org/hto#isRefedBy

hto:isRefedBy rdf:type :ObjectProperty ;
              
              :inverseOf hto:hasRef .



###  http://project-haystack.org/hto#locatedOn

hto:locatedOn rdf:type :ObjectProperty ;
              
              rdfs:domain hto:Point ;
              
              rdfs:range hto:Section ;
              
              rdfs:subPropertyOf <http://purl.oclc.org/NET/ssnx/ssn#isPropertyOf> .



###  http://project-haystack.org/hto#measuredOn

hto:measuredOn rdf:type :ObjectProperty ;
               
               rdfs:domain hto:Measurement ;
               
               rdfs:range hto:PhysicalBody .



###  http://project-haystack.org/hto#quantifies

hto:quantifies rdf:type :ObjectProperty ;
               
               rdfs:range hto:Measurement ;
               
               rdfs:domain hto:Point ;
               
               rdfs:subPropertyOf <http://purl.oclc.org/NET/ssnx/ssn#observes> .





